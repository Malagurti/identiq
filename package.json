{
  "name": "keyprint",
  "version": "0.1.0",
  "description": "A lightweight and efficient browser fingerprinting library",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "test": "jest",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "copy-lib": "node examples/copy-lib.js",
    "dev": "pnpm build && pnpm copy-lib && serve examples",
    "dev:watch": "rollup -c -w & pnpm copy-lib && serve examples",
    "prepublishOnly": "pnpm run build && pnpm run test:ci"
  },
  "keywords": [
    "fingerprint",
    "browser",
    "identity",
    "canvas",
    "hash"
  ],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.14.0",
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.2.5",
    "rollup": "^4.9.6",
    "serve": "^14.2.4",
    "ts-jest": "^29.1.2",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist"
  ]
}
